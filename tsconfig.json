{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "composite": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "types/**/*.ts",
    "expo-env.d.ts",
    "react-native.config.js",
    "app/_layout.jsx",
    "app/index.jsx",
    "assets/images/image_assets.js",
    ".expo/types/**/*.ts"
  ],
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/types",
    "apps/next/out",
    "apps/next/.next"
  ]
}
